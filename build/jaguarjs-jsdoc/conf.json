{
	"tags": {
		"allowUnknownTags" : true
	},
	"source": {
		"includePattern": ".+\\.js(doc)?$",
		"excludePattern": "(^|\\/|\\\\)_",
		"include": [
		    "../src/GeoBeans"
		    /*"externs/oli.js",
		    "externs/olx.js"*/
		]
	},
	"plugins": ["plugins/markdown"],
	"templates": {
		"cleverLinks": true,
		"monospaceLinks": true,
		"default": {
			"outputSourceFiles" : false
		},
		"applicationName": "Map5",
		"disqus": "",
		"googleAnalytics": "",
		"openGraph": {
			"title": "",
			"type": "website",
			"image": "",
			"site_name": "",
			"url": ""
		},
		"meta": {
			"title": "",
			"description": "",
			"keyword": ""
		},
		"linenums": false
	},
	"markdown": {
		"parser": "gfm",
		"hardwrap": true,
		"tags": ["examples"]
	},
	"opts": {
		"template" : "jaguarjs-jsdoc",
		"encoding": "utf8",
		"recurse": true,
		/*"tutorials": "../tutorials",*/
		"destination": "../api",
		"configure" : "./jaguarjs-jsdoc/conf.json"
	}
}
